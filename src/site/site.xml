<project name="Maven" xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.9</version>
  </skin>

  <publishDate format="dd MMMM yyyy" position="right"/>

  <bannerLeft>
    <name>Requestor GWT HTTP Client API</name>
  </bannerLeft>

  <custom>
    <fluidoSkin>
      <sideBarEnabled>true</sideBarEnabled>
      <googlePlusOne/>
    </fluidoSkin>
  </custom>

  <body>
    <links>
      <item name="GitHub" href="https://github.com/reinert/requestor" target="_blank"/>
    </links>

    <menu name="Examples">
      <item name="Showcase" href="examples/showcase" target="_blank"/>
    </menu>

    <menu name="Documentation">
      <item name="Wiki" href="https://github.com/reinert/requestor/wiki" target="_blank"/>
      <item name="Javadoc" href="javadoc/apidocs" target="_blank"/>
    </menu>

    <menu name="Community">
      <item name="Group" href="https://groups.google.com/forum/#!forum/requestor" target="_blank"/>
    </menu>

    <menu ref="reports"/>
  </body>
</project>